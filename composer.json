{
    "name": "zanysoft/laravel-zip",
    "description": "laravel-zip is the world's leading zip utility for file compression and backup.",
    "license": "MIT",
    "authors": [
        {
            "name": "Nguyen Truong Giang",
            "email": "nguyen.giang@rivercrane.com.vn",
            "homepage": "http://rivercrane.vn"
        }
    ],
    "keywords": [
        "laravel-zip",
        "laravel",
        "laravel5",
        "zip",
        "unzip",
        "extract",
        "backup",
        "ZipArchive",
        "multiple",
        "merge"
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Webike\\Zip\\ZipServiceProvider"
            ],
            "aliases": {
                "Zip": "Webike\\Zip\\ZipFacade"
            }
        }
    },
    "autoload": {
        "psr-4": {
           "Webike\\Zip\\": "src"
         }
    },
    "require": {
        "php": ">=5.6.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.0|^5.0",
        "scrutinizer/ocular": "^1.0"
    }
}
